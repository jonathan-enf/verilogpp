/**INST bus_keeper.v keeper7 --style=v3
  parameter WIDTH $bits(bus1_in)
**/
/*PPSTART*/
bus_keeper #(
  .WIDTH ($bits(bus1_in))
) keeper7 (
  .clk,  // input
  .reset_n,  // input
  .busNNN_valid,  // input
  .busNNN_in,  // input
  .busNNN_out  // output
);
/*PPSTOP*/


